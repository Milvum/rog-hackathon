{
  "extends": [
    "tslint:recommended",
    "tslint-react"
  ],
  "rules": {
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "member-ordering": false,
    "no-console": false,
    "no-unused-expression": false,
    "no-bitwise": false,
    "jsx-no-lambda": false,
    "jsx-no-multiline-js": false,
    "jsx-boolean-value": false,
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ]
  }
}