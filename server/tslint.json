{
    "extends": [
        "tslint:recommended"
    ],
    "rules": {
        "quotemark": [
            true,
            "double"
        ],
        "space-before-function-paren": false,
        "max-classes-per-file": false,
        "object-literal-sort-keys": false,
        "ordered-imports": false,
        "member-ordering": false,
        "variable-name": false,
        "no-console": [
            true,
            "log"
        ],
        "align": [
            true,
            "arguments",
            "elements",
            "members",
            "parameters",
            "statements"
        ],
        "indent": [
            true,
            "spaces",
            4
        ],
        "arrow-parens": [
            true,
            "ban-single-arg-parens"
        ],
        "no-unused-variable": [
            true
        ],
        "object-literal-shorthand": [
            true,
            "never"
        ],
        "interface-name": false
    }
}