{
  "name": "survey",
  "version": "1.0.0",
  "description": "Make random surveys",
  "main": "build/App.js",
  "scripts": {
    "prestart": "tsc",
    "start": "node build/App.js",
    "test": "jest --runInBand"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "moment": "^2.22.2",
    "querystring": "^0.2.0",
    "socket.io": "^2.1.1",
    "sqlite3": "^4.0.2",
    "uuid": "^3.3.2",
    "winston": "^3.0.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/cors": "^2.8.4",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.0",
    "@types/node": "^10.5.2",
    "@types/request": "^2.47.1",
    "@types/socket.io": "^1.4.36",
    "@types/sqlite3": "^3.1.3",
    "@types/uuid": "^3.4.4",
    "@types/winston": "^2.3.9",
    "jest": "^23.4.1",
    "ts-jest": "^23.0.1",
    "tslint": "^5.11.0",
    "typescript": "^2.9.2"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
